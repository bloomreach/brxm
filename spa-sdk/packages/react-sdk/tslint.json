{
  "extends": ["tslint-config-airbnb", "tslint-react"],
  "rules": {
    "function-name": [
      true,
      {
        "function-regex": "^[\\w$][\\w\\d]+$"
      }
    ],
    "import-name": false,
    "max-line-length": [true, 120],
    "variable-name": {
      "options": ["allow-pascal-case"]
    }
  }
}
